package by.frank.taskbar.progress.managers
{
    /**
     * Collection of TaskBarProgressManager#setProgressState
     * possible values.
     * @author Daniel R C Frank
     * @since 01/10/11
     * @version 1.0.0
     */
    public class ProgressStateType
    {
        public static const INDETERMINATE:int = 1;
        public static const NORMAL:int = 2;
        public static const ERROR:int = 3;
        public static const PAUSED:int = 4;
        public static const NO_PROGRESS:int = 0;
    }
}